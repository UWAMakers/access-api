{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "AUTH_SECRET",
    "authStrategies": [
      "jwt",
      "local",
      "label-printer"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://access.uwamakers.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "14d"
    },
    "local": {
      "usernameField": "email",
      "passwordField": "password"
    },
    "label-printer": {
      "service": "label-printers",
      "usernameField": "_id",
      "passwordField": "password"
    }
  },
  "ENV": "ENV",
  "mongodb": "MONGO_URI",
  "authEndpoint": "AUTH_ENDPOINT",
  "SMTP_HOST": "SMTP_HOST",
  "SMTP_USER": "SMTP_USER",
  "SMTP_PASSWORD": "SMTP_PASSWORD",
  "FEEDBACK_WEBHOOK": "FEEDBACK_WEBHOOK",
  "CATCH_NOTIFICATIONS": true,
  "CLIENT_DOMAIN": "http://localhost:8080",
  "mongoWhitelist": [
    "$eq",
    "$not",
    "$nor",
    "$and",
    "$elemMatch",
    "$all",
    "$size",
    "$exists",
    "$type",
    "$regexp",
    "$regex",
    "$options",
    "$text",
    "$search",
    "$language",
    "$caseSensitive",
    "$diacriticSensitive",
    "$month",
    "$year",
    "$hour",
    "$minute",
    "$seconds",
    "$milliseconds",
    "$week",
    "$dayOfYear",
    "$dayOfMonth",
    "$dayOfWeek"
  ]
}