{
  "host": "localhost",
  "port": 3030,
  "public": "./public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "tomatoPotato",
    "authStrategies": [
      "jwt",
      "local",
      "magic",
      "discord"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://access.uwamakers.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "14d"
    },
    "local": {
      "usernameField": "email",
      "passwordField": "password"
    },
    "magic": {
      "usernameField": "email",
      "passwordField": "password"
    },
    "oauth": {
      "redirect": "/",
      "origins": [
        "http://localhost:8080",
        "https://uwamakers.com"
      ],
      "discord": {
        "key": "DISCORD_CLIENT_ID",
        "secret": "DISCORD_CLIENT_SECRET",
        "custom_params": { "prompt": "none" },
        "scope": ["identify", "email"]
      }
    }
  },
  "CATCH_NOTIFICATIONS": true,
  "CLIENT_DOMAIN": "http://localhost:8080",
  "mongoWhitelist": [
    "$eq",
    "$not",
    "$nor",
    "$and",
    "$elemMatch",
    "$all",
    "$size",
    "$exists",
    "$type",
    "$regexp",
    "$regex",
    "$options",
    "$text",
    "$search",
    "$language",
    "$caseSensitive",
    "$diacriticSensitive",
    "$month",
    "$year",
    "$hour",
    "$minute",
    "$seconds",
    "$milliseconds",
    "$week",
    "$dayOfYear",
    "$dayOfMonth",
    "$dayOfWeek"
  ]
}